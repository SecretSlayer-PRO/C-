/*Find the average of 3 numbers using C++ code
The 3rd to 5th line is called "Header file"*/
#include <iostream>
using namespace std;
int main() {
	int num1, num2, num3, average;
	cout<<"Enter your first number:";
	cin>>num1;
	cout<<"Enter your second number:";
	cin>>num2;
	cout<<"Enter your third number:";
	cin>>num3;
	average=(num1+num2+num3)/3;
	cout<<"The average of these three numbers is:"<<average<<endl;
	return 0;
}
